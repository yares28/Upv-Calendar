{
  "name": "upv-calendar-backend",
  "version": "1.0.0",
  "description": "Backend for UPV Calendar app",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "db:init": "ts-node src/scripts/setupDB.ts schema",
    "db:seed": "ts-node src/scripts/setupDB.ts seed",
    "db:reset": "ts-node src/scripts/setupDB.ts reset",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "calendar",
    "api",
    "upv"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.17.30",
    "@types/pg": "^8.11.13",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
